{
    "name": "two-thirds/eloquent-traits",
    "description": "",
    "keywords": [],
    "homepage": "https://www.gitlab.com/two-thirds/eloquent-traits.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Hoff",
            "email": "jhoff@twothirds.io"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "5.*|6.*",
        "illuminate/support": "5.*|6.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~8.0",
        "two-thirds/artisan-anywhere": "^1.2",
        "two-thirds/laravel-test-suite": "^4.1"
    },
    "suggest": {
        "anthonymartin/geo-location": "Required to use geoLocation trait with a SqlLite database"
    },
    "config": {
        "secure-http": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "TwoThirds\\EloquentTraits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoThirds\\Testing\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TwoThirds\\EloquentTraits\\EloquentTraitsServiceProvider"
            ]
        }
    }
}
